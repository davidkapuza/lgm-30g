{
  "name": "user-service",
  "version": "0.0.0",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "bundle": "rimraf dist && esbuild --outdir=dist --sourcemap --bundle --platform=node --target=node20.10.0 ./src/index.ts",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "dev": "nodemon --watch src --exec 'npm run bundle && npm run start'",
    "debug": "npm run bundle && node --enable-source-maps --inspect-brk ./dist/index.js",
    "debug:test": "node --inspect-brk ./node_modules/.bin/jest --runInBand .",
    "start": "node --enable-source-maps ./dist/index.js",
    "test": "jest --runInBand ."
  },
  "devDependencies": {
    "@babel/core": "7.25.7",
    "@babel/preset-env": "7.25.7",
    "@babel/preset-typescript": "7.25.7",
    "@tsconfig/node20": "20.1.4",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/node": "20.16.10",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "esbuild": "0.21.3",
    "nodemon": "3.1.7",
    "rimraf": "5.0.10",
    "typescript": "5.6.2"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^5.20.0",
    "zod": "^3.23.8"
  }
}
